<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Full Regression Suite" parallel="none">

    <test name="Main Tests" preserve-order="true">
        <classes>
            <class name="runners.RunTest"/>
